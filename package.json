{
  "name": "cql-syntax",
  "displayName": "CQL Syntax",
  "description": "CQL Syntax Highlighting for .cql files",
  "version": "0.0.2",
  "publisher": "amata1219",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "cql",
      "aliases": ["CQL", "cql"],
      "extensions": [".cql"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "cql",
      "scopeName": "source.cql",
      "path": "./syntaxes/cql.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amata1219/cql-syntax.git"
  }
}
